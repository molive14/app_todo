<h1>List Items</h1>
<ul> 
    <% @todos.each do |todo| %>
        <li class= "todo_item">
            <input type="checkbox">
            <a href= "/todo/show/<%=todo.id%>"> <%=todo.description%> 
            <span class="pomodoro-estimate"><%=todo.pomodoro_estimate%></span>
            </a>
        </li>
    <%end%>
</ul>
<a class="button" href="/todo/new"id="add-new-todo-button">ADD</a>
